<Window x:Class="Game_of_Life_old.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game_of_Life_old"
        mc:Ignorable="d" Name="Main" DataContext="{Binding ElementName=Main}"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="25"/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="8*"/>
            <RowDefinition Height="25"/>
        </Grid.RowDefinitions>
        <Frame  Grid.Row="1" Grid.Column="1" Content="{Binding View}" NavigationUIVisibility= "Hidden"/>
        <StackPanel Grid.Column="1" Grid.Row="2" Orientation="Horizontal">
            
        </StackPanel>
    </Grid>


</Window>
